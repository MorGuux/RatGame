<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="249.0" prefWidth="360.0" styleClass="BackgroundPane" stylesheets="@../../../../assets/css/dark_theme.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.menu.dependant.level.type.LevelTypeForm">
   <top>
      <Label alignment="BOTTOM_CENTER" prefHeight="37.0" prefWidth="190.0" text="Enter a username!" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </BorderPane.margin>
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
         <styleClass>
            <String fx:value="AltFontA" />
            <String fx:value="MediumText" />
         </styleClass>
      </Label>
   </top>
   <center>
      <TextField fx:id="usernameInputField" alignment="CENTER" promptText="Username..." styleClass="ContentModule" BorderPane.alignment="CENTER">
         <padding>
            <Insets bottom="6.0" left="6.0" right="6.0" top="6.0" />
         </padding>
         <BorderPane.margin>
            <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
         </BorderPane.margin>
      </TextField>
   </center>
   <bottom>
      <HBox alignment="CENTER" spacing="10.0" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </BorderPane.margin>
         <children>
            <Button mnemonicParsing="false" onAction="#onCustomLevelClicked" text="Custom Level">
               <font>
                  <Font size="13.0" />
               </font>
               <padding>
                  <Insets bottom="6.0" left="6.0" right="6.0" top="6.0" />
               </padding>
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
               <styleClass>
                  <String fx:value="AltFontB" />
                  <String fx:value="ClickEffect" />
                  <String fx:value="ContentModule" />
                  <String fx:value="DefaultText" />
               </styleClass>
            </Button>
            <Button layoutX="136.0" layoutY="20.0" mnemonicParsing="false" onAction="#onDefaultLevelClicked" text="Default Level">
               <font>
                  <Font size="13.0" />
               </font>
               <padding>
                  <Insets bottom="6.0" left="6.0" right="6.0" top="6.0" />
               </padding>
               <styleClass>
                  <String fx:value="AltFontB" />
                  <String fx:value="ClickEffect" />
                  <String fx:value="ContentModule" />
                  <String fx:value="DefaultText" />
               </styleClass>
            </Button>
         </children>
      </HBox>
   </bottom>
</BorderPane>
